{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      },
      {
        "src": "routes/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/auth",
        "dest": "routes/auth.js"
      },
      {
        "src": "/production",
        "dest": "routes/production.js"
      },
      {
        "src": "/report",
        "dest": "routes/report.js"
      },
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ]
  }
  